{
  "name": "ourvoice-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port=3010 --host",
    "build": "run-p type-check build-only",
    "build:ci": "run-s type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:cli": "cypress run --e2e",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:ci": "vue-ts --incremental false --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "graphql:generate": "graphql-codegen --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/js-yaml": "^4.0.9",
    "@unhead/vue": "^1.8.13",
    "@vue/apollo-composable": "4.0.2",
    "@vueform/multiselect": "^2.6.6",
    "@vueuse/core": "^10.9.0",
    "crypto-js": "^4.1.1",
    "cypress-file-upload": "^5.0.8",
    "date-fns": "^2.30.0",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.1.7",
    "supertokens-web-js": "^0.9.1",
    "tailwind-merge": "^1.14.0",
    "unique-names-generator": "^4.7.1",
    "vee-validate": "^4.12.6",
    "vite-plugin-vue-markdown": "^0.23.8",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/crypto-js": "^4.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.23",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "cypress": "^12.17.4",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "vite": "^4.3.9",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  }
}
