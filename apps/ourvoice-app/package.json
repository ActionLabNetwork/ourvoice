{
  "name": "ourvoice-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port=3010 --host",
    "build": "run-p type-check build-only",
    "build:ci": "run-s type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:cli": "cypress run --e2e",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:ci": "vue-ts --incremental false --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "graphql:generate": "graphql-codegen --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.1.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/js-yaml": "^4.0.9",
    "@unhead/vue": "^1.8.10",
    "@vue/apollo-composable": "4.0.0",
    "@vueform/multiselect": "^2.6.6",
    "@vueuse/core": "^10.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "crypto-js": "^4.2.0",
    "cypress-file-upload": "^5.0.8",
    "date-fns": "^3.3.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lucide-vue-next": "^0.314.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.3.2",
    "supertokens-web-js": "^0.8.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "unique-names-generator": "^4.7.1",
    "vee-validate": "^4.12.4",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@rushstack/eslint-patch": "^1.7.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/crypto-js": "^4.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  }
}
