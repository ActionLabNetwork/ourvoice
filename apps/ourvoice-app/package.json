{
  "name": "ourvoice-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port=3010 --host",
    "build": "run-p type-check build-only",
    "build:ci": "run-s type-check build-only",
    "start:prod": "vite preview --port=3010 --host",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:cli": "cypress run --e2e",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:ci": "vue-ts --incremental false --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "graphql:generate": "graphql-codegen --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
    "@tsconfig/node18": "18.2.4",
    "@types/js-yaml": "^4.0.5",
    "@unhead/vue": "^1.1.28",
    "@vue/apollo-composable": "4.0.0-beta.7",
    "@vueform/multiselect": "^2.6.2",
    "@vueuse/core": "^10.1.2",
    "crypto-js": "^4.1.1",
    "cypress-file-upload": "^5.0.8",
    "date-fns": "3.6.0",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.1.4",
    "supertokens-web-js": "0.13.0",
    "tailwind-merge": "2.4.0",
    "unique-names-generator": "^4.7.1",
    "vee-validate": "^4.10.2",
    "unplugin-vue-markdown": "0.26.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "22.1.0",
    "@vitejs/plugin-vue": "5.1.2",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "^10.4.14",
    "cypress": "13.13.2",
    "eslint": "9.8.0",
    "eslint-plugin-cypress": "3.4.0",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "24.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "3.3.3",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "vite": "5.3.5",
    "vitest": "2.0.5",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "2.0.29"
  }
}
